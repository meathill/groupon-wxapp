page
  display flex
  flex-direction column

.avatar,
.get-user-info
  width 300rpx !important
  height 300rpx
  border-radius 150rpx
  margin 0 auto
  border 2px solid white

.get-user-info
  padding 0
  line-height 300rpx
  font-size 48rpx
  font-weight 200

.header
  padding-bottom 40rpx
  margin 0 20rpx
  border-bottom 4rpx solid #DDD

.photo-list
  border-top 2rpx solid white
  margin 0 20rpx
  flex 1
